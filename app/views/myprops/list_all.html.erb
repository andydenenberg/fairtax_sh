<div class='row'>
	<div class='span6'>
		<ul class="breadcrumb">
		  <li><a href="#">Comp</a></li> |
		  <li><a href="#">Show List Members</a></li>
		</ul>
	</div>
		<div class='span6'>

			<div class="btn-group pull-right">
			<%= link_to "Show List Members", list_all_myprops_path, :class => 'btn active' %>
			<%= link_to "Create New List", myprops_path, :class => 'btn' %>
			</div>
		</div>
</div>

<div class='row'>
	<div class='span11 offset1'>
		<div class="page-header">
		  <h3>List Members
		  </h3>
		</div>

		<table class="table table-striped table-bordered table-condensed">
		  <thead>
		    <tr>
		      <th>ID</th>
			  <th>PIN</th>
			  <th>Address</th>
			  <th>City</th>
			  <th>Status</th>
			  <th>User ID</th>
			  <th>List Number</th>
		    </tr>
		  </thead>
		  <tbody>
		<% @all.each do |prop| %>
		     <tr>
		       <td><%= prop.id %> <%= show_mine(prop.id).html_safe %></td>
			   <td><%= prop.pin %></td>
			   <td><%= link_to prop.street, myprop_path(prop.id) %></td>
			   <td><%= prop.city %></td>
			   <td><%= prop.status %></td>
			   <td><%= xslate_user(prop.user_id) %></td>
			   <td><%= prop.list_num %></td>
		      </tr>
		    <% end %>
		  </tbody>
		</table>
	</div>
</div>
<br><br><br><br>
