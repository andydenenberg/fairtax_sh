		<table class="table table-striped table-bordered table-condensed">
		  <thead>
		    <tr>
			  <th>Owner's Name</th>
			  <th><%= link_to 'Street_Address' , comp_block_summary_properties_path(:id => @primary_id.id, :sort => 'properties.street desc', :list_num => @list_num) %> </th>
		      <th><%= link_to 'Classification', comp_block_summary_properties_path(:id => @primary_id.id, :sort => 'properties.class_code desc', :list_num => @list_num) %></th>
			  <th><%= link_to 'Age', comp_block_summary_properties_path(:id => @primary_id.id, :sort => 'details.age desc') %></th>
			  <th><%= link_to 'Building Size', comp_block_summary_properties_path(:id => @primary_id.id, :sort => 'details.building_size desc', :list_num => @list_num) %></th>
			  <th><%= link_to 'Land Size', comp_block_summary_properties_path(:id => @primary_id.id, :sort => 'details.land_size desc', :list_num => @list_num) %></th>
			  <th><%= link_to 'Assessed Value', comp_block_summary_properties_path(:id => @primary_id.id, :sort => 'properties.assessed_value desc', :list_num => @list_num) %></th>
		    </tr>
		 </thead>
		
		  <tbody>
		<% @props.each_with_index do |prop,i| %>
		     <tr>
			  <td><%= prop.owner.cc_name %></td>
			   <td><%= link_to prop.street, property_path(prop.id) %></td>
				<td><a href="/properties/<%= prop.id %>"  rel="popover" data-content="<%= class_code_lu(prop.class_code) %><br>" data-original-title="Class Code"><%= prop.class_code %></td>
			   <td style='text-align:right' ><%= prop.detail.age %></td>
			   <td style='text-align:right' ><%= number_with_delimiter(prop.detail.building_size, :delimiter => ",") %></td>
			   <td style='text-align:right' ><%= number_with_delimiter(prop.detail.land_size, :delimiter => ",") %></td>
			   <td style='text-align:right' >$<%= number_with_delimiter(prop.assessed_value, :delimiter => ",") %></td>
			
		      </tr>
		    <% end %>		
		
		  </tbody>
		
		</table>

<br><br><br><br>
