<h1>/Class Code</h1>

<p>
  <b>Cc</b><br>
  <%= @class_code.cc %>
</p>

<p>
  <b>Description</b><br>
  <%= @class_code.description %>
</p>

<p>
  <b>Orig desc</b><br>
  <%= @class_code.orig_desc %>
</p>

<p>
  <b>Category</b><br>
  <%= @class_code.category %>
</p>

<p>
  <b>Comments</b><br>
  <%= @class_code.comments %>
</p>

<p>
  <b>Value</b><br>
  <%= @class_code.value %>
</p>

<div class="form-actions">
  <%= link_to 'Back', class_codes_path, :class => 'btn'  %>
  <%= link_to 'Edit', edit_class_code_path(@class_code), :class => 'btn' %>
  <%= link_to 'Delete', class_code_path(@class_code), :method => 'delete', :confirm => 'Are you sure?', :class => 'btn btn-danger' %>
</div>
